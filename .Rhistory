server <- function(input, output, session) {
output$main_plot <- renderPlot({
p <- ggplot(df_inequality, aes(x=Year))+
coord_cartesian(clip = "off")+
scale_y_continuous(position= "right",
labels = c(0, 10, 20, 30, 40, 50, 60),
breaks = c(0, 10, 20, 30, 40, 50, 60),
limits = c(0, 60))+
scale_x_continuous(limits = c(1980, 2020),
breaks = c(1980, 1990, 2000, 2010, 2018),
labels = c(1980, 1990, 2000, 2010, 2018))+
ggtitle(label = "Divergence",
subtitle = "Share of top decile and bottom half in total wealth, %")+
labs(caption = "Source: World inequality report 2018")+
my_theme
if (input$china_top_10) {
p <- p + geom_line(aes(y=china_top_10), size=1.2, colour="#9e386f")+
geom_point(aes(y=china_top_10), size=3, alpha=.005)}
if (input$india_top_10) {
p <- p + geom_line(aes(y=india_top_10), size=1.2, colour="#0073a1")+
geom_point(aes(y=india_top_10), size=3, alpha=.005)}
if (input$us_top_10) {
p <- p + geom_line(aes(y=us_top_10), size=1.2, colour="#cf8ab2")+
geom_point(aes(y=us_top_10), size=3, alpha=.005)}
if (input$russia_top_10) {
p <- p + geom_line(aes(y=russia_top_10), size=1.2, colour="#008a5a")+
geom_point(aes(y=russia_top_10), size=3, alpha=.005)}
if (input$europe_top_10) {
p <- p + geom_line(aes(y=europe_top_10), size=1.2, colour="#57a7d8")+
